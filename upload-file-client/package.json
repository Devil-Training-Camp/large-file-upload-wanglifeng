{
  "name": "upload-file-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint src/**/*.{vue,ts}",
    "eslint:fix": "eslint src/**/*.{vue,ts} --fix",
    "prettier-format": "prettier --write \"src/**/*.{html,vue,ts}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss,sass,vue}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,sass,vue}\" --fix",
    "lint:lint-staged": "lint-staged",
    "prepare": "cd .. && husky upload-file-client/.husky"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "pnpm run eslint",
      "pnpm run prettier-format"
    ]
  },
  "dependencies": {
    "element-plus": "^2.7.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "stylelint": "^16.3.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.2.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.7.0",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}